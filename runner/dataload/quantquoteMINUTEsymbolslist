rm -r ~/.zipline/data_to_ingest/minute/*
rm -r ~/.zipline/minute_data_combined/*

declare -a symbols=("UVXY" "SPY" "TLT" "SVXY")

for s in "${symbols[@]}"
do
    b1="table_${s,,}.csv"
    find "${HOME}/.zipline/minute" -name $b1 -exec cat {} >>"${HOME}/.zipline/minute_data_combined/$b1" \;
#    python fix_quantquote_csv.py ${HOME}/.zipline/minute_data_combined ${HOME}/.zipline/data_to_ingest/minute ${HOME}/.zipline/splits $s
done

